# Jenkins API Credentials
# Copy this to .env.jenkins and fill in your credentials

# Jenkins URL (without trailing slash)
JENKINS_URL=https://jenkins.arpansahu.space

# Jenkins username for API access
JENKINS_USER=arpansahu

# Jenkins API token
# Generate from: Click your username (top-right) → Configure → API Token → Add new Token
JENKINS_API_TOKEN=your_api_token_here
